{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "logs:CreateLogGroup"
            ],
            "Resource": "arn:aws-cn:logs:%%AWS_REGION%%:%%AWS_ACCOUNT_ID%%:log-group:/aws/lambda/%%CLUSTER_ID%%*",
            "Effect": "Allow",
            "Sid": "CloudWatchLogsPermissions"
        },
        {
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": "arn:aws-cn:logs:%%AWS_REGION%%:%%AWS_ACCOUNT_ID%%:log-group:/aws/lambda/%%CLUSTER_ID%%*:log-stream:*",
            "Effect": "Allow",
            "Sid": "CloudWatchLogStreamPermissions"
        },
        {
            "Action": [
                "iam:ListRoles"
            ],
            "Resource": "*",
            "Effect": "Allow",
            "Sid": "AOSSIAMPermissions"
        },
        {
            "Action": [
                "aoss:*"
            ],
            "Resource": "arn:aws-cn:aoss:%%AWS_REGION%%:%%AWS_ACCOUNT_ID%%:collection/*",
            "Effect": "Allow",
            "Sid": "AOSSPermissions"
        }
    ]
}
